# Dinner Bot 환경 설정
# .env.example을 .env로 복사 후 실제 값 입력

# Telegram 설정
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Google Gemini API (필수!)
# https://aistudio.google.com/apikey 에서 발급
GEMINI_API_KEY=your_gemini_api_key

# 식단 추천 스케줄 (토양체질 매일 17:30)
MEAL_SEND_HOUR=17
MEAL_SEND_MINUTE=30
TIMEZONE=Asia/Seoul

# 서버 설정
PORT=8080
LOG_LEVEL=INFO

# 잔반 재료 (콤마로 구분)
LEFTOVER_INGREDIENTS=
